# DevStack API Configuration
# Copy this file to .env and update with your actual values

# Your DevStack API key (get from DevStack dashboard)
DEVSTACK_API_KEY=ds_test_YOUR_API_KEY_HERE

# Your DevStack project ID
DEVSTACK_PROJECT_ID=YOUR_PROJECT_ID_HERE

# Your DevStack organization ID
DEVSTACK_ORGANIZATION_ID=YOUR_ORGANIZATION_ID_HERE

# DevStack API endpoint
# For local development:
DEVSTACK_API_ENDPOINT=http://localhost:5001/api/v1

# For Android emulator:
# DEVSTACK_API_ENDPOINT=http://10.0.2.2:5001/api/v1

# For iOS simulator (replace with your host IP):
# DEVSTACK_API_ENDPOINT=http://192.168.1.194:5001/api/v1

# For production:
# DEVSTACK_API_ENDPOINT=https://api.devstack.com/api/v1