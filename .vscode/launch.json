  "version": "0.2.0",
  "configurations": [
    {
      "name": "example",
      "cwd": "packages/voo_logging/example",
      "request": "launch",
      "type": "dart",
      "args": ["--print-dtd"]
    },
    {
      "name": "(sim) logging dev tools",
      "cwd": "tools/voo_logging_devtools_extension",
      "request": "launch",
      "type": "dart",
      "args": [
        "--dart-define=use_simulated_environment=true",
        "--web-experimental-hot-reload"
      ],
      "env": {
        "DART_VM_OPTIONS": "--enable-vm-service"
      },
      "program": "lib/main.dart"
    }
  ]
}
